<div class="container mt-4">
    <div class="row">
        <div class="offset-2"></div>
        {{#each user}}
        <div class="col-md-4">
            <img src="{{profile}}" class="img-fluid rounded-circle">
        </div>
        <div class="col-md-6 text-white">
            <h3>{{name}}</h3>
            <h4>{{email}}</h4>
            <h4>{{address}}</h4>
            <h4>{{contact}}</h4>
        </div>
        {{/each}}
    </div>
</div>